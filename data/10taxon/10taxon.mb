begin mrbayes;
  execute 10taxon.nex;
  outgroup 1;
  prset statefreqpr=fixed(equal);
  prset Brlens=unconstrained:exponential(10.0);
  lset nst=1;
  set seed=1;
  mcmcp ngen=5000000 samplefreq=1000 printfreq=1000 nruns=1;
  mcmcp nruns=1 nchains=1 savebrlens=yes;
  propset ParsSPR(Tau,V)$prob=0;
  propset ExtSPR(Tau,V)$prob=0;
  propset ExtTBR(Tau,V)$prob=0;
  mcmc file=mb_10taxon;
  sump file=mb_10taxon nruns=1;
  sumt file=mb_10taxon nruns=1;
end;
